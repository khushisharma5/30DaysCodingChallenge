class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        
        vector<int> ans;
        unordered_map<int, int> count;

        for(int num:nums){
            count[num]++;
        }

        for(int i=1;i<=nums.size(); i++){
            if(count[i]==2){
                ans.push_back(i);
            }
        }
        return ans;
    }
};
